    "description": "Provides a form ordering support for your Symfony project.",
    "keywords": [ "form", "order" ],
    "license": "MIT",
    "authors": [
        {
            "name": "Eric GELOEN",
            "email": "geloen.eric@gmail.com"
        },
        {
            "name": "Nikita Loges",
            "email": "nl@tenolo.de"
        },
        {
            "name": "Dennis Hebbeler",
            "email": "dh@tenolo.de"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",
        "symfony/framework-bundle": "^2.8|^3.4|^4.0",
        "tenolo/form-ordered": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "symfony/phpunit-bridge": "^2.8|^3.4|^4.0"
