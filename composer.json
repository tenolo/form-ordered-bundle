{
    "name": "tenolo/form-ordered-bundle",
    "description": "Provides a form ordering support for your Symfony project.",
    "keywords": [
        "form",
        "order"
    ],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Eric GELOEN",
            "email": "geloen.eric@gmail.com"
        },
        {
            "name": "Nikita Loges",
            "email": "nl@tenolo.de"
        },
        {
            "name": "Dennis Hebbeler",
            "email": "dh@tenolo.de"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",

        "symfony/framework-bundle": "^3.4|^4.0",
        "symfony/config": "^3.4|^4.0",
        "symfony/dependency-injection": "^3.4|^4.0",
        "symfony/http-kernel": "^3.4|^4.0",

        "tenolo/form-ordered": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "symfony/phpunit-bridge": "^2.8|^3.4|^4.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.tenolo.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Tenolo\\Bundle\\FormOrderedBundle\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
